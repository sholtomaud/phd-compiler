{
    "name": "phd",
    "version": "1.0.1",
    "description": "pandoc app",
    "main": "index.js",
    "bin": {
        "phd": "./index.js"
    },
    "directories": {
        "test": "test"
    },
    "scripts": {
        "watch": "node ./index.js -d ./thesis",
        "build": "browserify -g uglifyify  -d -p [minifyify --map bundle.map.json --output bundle.map.json ] scripts/index.js > ../phdnode.github.io/index.browser.js & stylus -r -u nib < ~/dev/pandoc/stylus/index.styl | cssmin > ../phdnode.github.io/style.css | cssmin ",
        "dist": "npm version patch -m \"Bumped to %s\"",
        "postversion": "git push && git push --tags && npm run build",
        "test": "test",
        "debug": "DEBUG=* node index.js -d ./thesis"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/phdnode/pandoc.git"
    },
    "keywords": [
        "traffic",
        "lights"
    ],
    "author": "Sholto Maud",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/phdnode/pandoc/issues"
    },
    "homepage": "https://phdnode.github.io/pandoc/#",
    "dependencies": {
        "canvas": "^1.6.6",
        "commander": "^2.9.0",
        "convnetjs": "^0.3.0",
        "crel": "^3.1.0",
        "crelns": "^1.0.1",
        "d3": "^4.10.0",
        "d3-dsv": "^1.0.5",
        "d3-timer": "^1.0.5",
        "debug": "^3.0.0",
        "fastn": "^2.8.0",
        "file-server": "^1.0.5",
        "fs-extra": "^4.0.2",
        "google-auth-library": "^9.6.3",
        "googleapis": "^20.1.0",
        "jsdom": "^11.1.0",
        "kgo": "^4.0.3",
        "level": "^1.5.0",
        "leveldown": "^1.4.6",
        "levelup": "^1.3.2",
        "mathjax": "^2.7.0",
        "mime": "^1.3.6",
        "minimist": "^1.2.0",
        "ml-matrix": "^5.0.1",
        "nanocomponent": "^6.1.0",
        "node-gyp": "^3.6.2",
        "npm-check": "^5.4.5",
        "puppeteer": "^0.9.0",
        "shasum": "^1.0.2",
        "slug": "^0.9.1",
        "split": "^1.0.0",
        "topojson": "^3.0.0",
        "webpage": "^0.3.0",
        "xkcdplot": "^1.1.0"
    },
    "devDependencies": {
        "d3-geo": "^1.6.4",
        "minifyify": "^7.3.4",
        "nib": "^1.1.2",
        "rootify": "^1.1.0",
        "rooty": "^2.0.0",
        "uglify-js": "^3.0.27"
    },
    "browserify": {
        "debug": true,
        "transform": [
            "rootify",
            "babelify"
        ]
    },
    "jshintConfig": {
        "esversion": 6,
        "undef": true,
        "unused": true,
        "node": true,
        "forin": true,
        "maxcomplexity": true,
        "nonbsp": true,
        "singleGroups": true,
        "strict": true,
        "trailingcomma": true,
        "varstmt": true,
        "browserify": true
    },
    "babelify": {
        "presets": [
            "es2015"
        ],
        "comments": false,
        "env": {
            "production": {
                "presets": [
                    "babili"
                ]
            },
            "development": {
                "presets": [
                    "babili"
                ]
            }
        }
    },
    "rootify": {
        "rootPath": "./scripts"
    }
}
